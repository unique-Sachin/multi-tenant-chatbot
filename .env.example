# Redis Configuration
REDIS_URL=redis://localhost:6379

# Copy this to .env and fill in your values
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Milvus/Zilliz for vector storage (replaces Pinecone)
MILVUS_URI=https://your-cluster.zillizcloud.com
MILVUS_TOKEN=your_zilliz_api_token_here
MILVUS_COLLECTION=your_milvus_collection_name_here

# OpenAI for embeddings
OPENAI_API_KEY=your_openai_api_key_here

# Chatbot Behavior Configuration
MIN_SCOPE_SIM=0.4

# Streamlit Authentication
STREAMLIT_AUTH_SHARED_SECRET=your_streamlit_auth_secret_here

# Optional: Cohere for reranking
COHERE_API_KEY=your_cohere_api_key_here